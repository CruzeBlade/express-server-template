{
    "name": "express-server-template",
    "version": "1.0.0",
    "description": "This is a project template for an node.js express server written in typescript with mongodb as database.",
    "main": "dist/src/main.js",
    "scripts": {
        "dev": "nodemon",
        "build": "tsc",
        "lint": "eslint --ext .js,.ts",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "serve": "node dist/src/main.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "mongodb": "^6.8.0",
        "mongoose": "^8.6.1",
        "uuid": "^10.0.0"
    }
}
